{
  "name": "project-starter",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@fontsource/roboto": "^4.5.8",
    "@mikro-orm/cli": "^5.6.6",
    "@mikro-orm/core": "^5.6.6",
    "@mikro-orm/knex": "^5.6.6",
    "@mikro-orm/migrations": "^5.6.6",
    "@mikro-orm/postgresql": "^5.6.6",
    "@mui/icons-material": "^5.11.0",
    "@mui/material": "^5.11.4",
    "@types/react-table": "^7.7.14",
    "pg": "^8.8.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "sass": "^1.57.1",
    "ts-node": "^10.9.1",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/plugin-syntax-flow": "^7.18.6",
    "@babel/plugin-transform-react-jsx": "^7.20.7",
    "@rollup/plugin-terser": "^0.3.0",
    "@testing-library/dom": "^8.19.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^29.2.5",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "eslint": "^8.31.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.4",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "git-commit-msg-linter": "^4.5.0",
    "husky": "^8.0.3",
    "jest": "^29.3.1",
    "lint-staged": "^13.1.0",
    "nodemon": "^2.0.20",
    "postcss": "^8.4.21",
    "prettier": "^2.8.3",
    "typescript": "^4.9.4",
    "validate-branch-name": "^1.3.0"
  },
  "scripts": {
    "build": "react-scripts build",
    "deploy": "",
    "check-types": "tsc",
    "start": "react-scripts start",
    "test": "react-scripts test --watchAll=false --findRelatedTests --bail",
    "eject": "react-scripts eject",
    "stage": "serve -s build",
    "watch": "tsc -watch",
    "lint-js": "eslint ./*.js --fix-dry-run",
    "lint-ts": "eslint src/**/*.ts --fix-dry-run",
    "lint-tsx": "eslint src/**/*.tsx --fix-dry-run",
    "lint:fix-js": "eslint ./*.js --fix",
    "lint:fix-ts": "eslint src/**/*.ts --fix",
    "lint:fix-tsx": "eslint src/**/*.tsx --fix",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --fix-dry-run",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "prepare": "husky install"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "npm lint",
      "npm prettier --write --ignore-unknown",
      "npm test"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "validate-branch-name": {
    "pattern": "^(master|main|develop){1}$|^(feature|fix|hotfix|release)/.+$",
    "errorMsg": "Invalid branch name. Please use (master|main|develop) or (feature|fix|hotfix|release) ex. develop/ticket-id/branch-name or develop/branch-name or feature/ticket-id/branch-name or feature/branch-name."
  },
  "jest": {},
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
